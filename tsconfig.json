{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@tasj=k/*": [
        "src/task/*"
      ]
    }
  }
}